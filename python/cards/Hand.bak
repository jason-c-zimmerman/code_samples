class Hand:

	hand = []

	def __init__(self, size=7):
		self.size = size

	def push(self, card):
		if card is not None:
			self.hand.append(card)
		
	def is_full(self):
		if len(self.hand) < self.size:
			return False
		return True

	def print_hand(self):
		for card in self.hand:
			print card.rank.name, "of", card.suit.name  